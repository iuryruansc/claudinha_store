<nav class="navbar navbar-expand-lg navbar-dark bg-primary">
    <a class="navbar-brand" href="/">
        CSDB
    </a>
    <ul class="navbar-nav me-auto">
        <% navLinks.forEach(link=> { %>
            <% if (link.dropdown) { %>
                <li class="nav-item dropdown">
                    <button class="nav-link dropdown-toggle" id="navbarDropdown-<%= link.label %>" type="button"
                        data-bs-toggle="dropdown" aria-expanded="false">
                        <%= link.label %>
                    </button>
                    <ul class="dropdown-menu" aria-labelledby="navbarDropdown-<%= link.label %>">
                        <% link.dropdown.forEach(item=> { %>
                            <li><a class="dropdown-item" href="<%= item.href %>">
                                    <%= item.label %>
                                </a></li>
                            <% }) %>
                    </ul>
                </li>
            <% } else { %>
                <li class="nav-item">
                    <a class="nav-link" href="<%= link.href %>">
                        <%= link.label %>
                    </a>
                </li>
            <% } %>
        <% }) %>
    </ul>
</nav>